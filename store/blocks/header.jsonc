{
	"header": {
		"blocks": ["header-layout.desktop", "header-layout.mobile"]
	},
	"header.full": {
		"blocks": ["header-layout.desktop", "header-layout.mobile"]
	},
	// DESKTOP
	"header-layout.desktop": {
		"children": ["sticky-layout#desktop"]
	},
	"sticky-layout#desktop": {
		"children": [
			"flex-layout.row#main",
			"flex-layout.row#menu",
			"flex-layout.row#tabs"
		]
	},
	"sticky-layout#mobile": {
		"children": ["flex-layout.row#mobile", "flex-layout.row#tabs"]
	},
	"flex-layout.row#main": {
		"props": {
			"horizontalAlign": "center",
			"verticalAlign": "center",
			"preventHorizontalStretch": true,
			"preventVerticalStretch": true,
			"fullWidth": true
		},
		"children": [
			"flex-layout.col#logo-desktop",
			"flex-layout.col#spacer",
			"search-bar",
			"minicart.v2"
		]
	},
	"flex-layout.col#logo-desktop": {
		"props": {
			"verticalAlign": "middle"
		},
		"children": ["logo"]
	},
	"flex-layout.row#menu": {
		"children": ["menu"]
	},
	"flex-layout.row#tabs": {
		"children": ["tabs"],
		"props": {
			"blockClass": "tabs"
		}
	},
	"menu": {
		"props": {
			"items": [
				{
					"id": "menu-item-promocao",
					"type": "custom",
					"iconId": null,
					"highlight": false,
					"itemProps": {
						"type": "internal",
						"href": "#",
						"noFollow": false,
						"tagTitle": "PROMOÇÃO",
						"text": "PROMOÇÃO"
					}
				},
				{
					"id": "menu-item-gremio",
					"type": "custom",
					"iconId": null,
					"highlight": false,
					"itemProps": {
						"type": "internal",
						"href": "#",
						"noFollow": false,
						"tagTitle": "GRÊMIO",
						"text": "GRÊMIO"
					}
				},
				{
					"id": "menu-item-internacional",
					"type": "custom",
					"iconId": null,
					"highlight": false,
					"itemProps": {
						"type": "internal",
						"href": "#",
						"noFollow": false,
						"tagTitle": "INTERNACIONAL",
						"text": "INTERNACIONAL"
					}
				}
			]
		}
	},
	// MOBILE
	"header-layout.mobile": {
		"children": ["sticky-layout#mobile"]
	},
	"flex-layout.row#mobile": {
		"children": [
			"drawer",
			"flex-layout.col#logo-mobile",
			"minicart.v2"
		],
		"props": {
			"preventHorizontalStretch": true,
			"preserveLayoutOnMobile": true,
			"justifyContent": "space-between",
			"blockClass": "mobile-main"
		}
	},
	"drawer": {
		"children": []
	},
	"flex-layout.col#logo-mobile": {
		"props": {
			"verticalAlign": "middle"
		},
		"children": ["logo"]
	},
	// UNIVERSAL
	"logo": {
		"props": {
			"title": "Logo",
			"href": "/",
			"url": "https://maratonavirtual.s3.sa-east-1.amazonaws.com/upmedal.png",
			"width": "150"
		}
	},
	"flex-layout.col#spacer": {
		"props": {
			"width": "grow"
		}
	}
}
